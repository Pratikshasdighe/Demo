{
  "name": "expo-template-typescript-jest",
  "version": "1.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__"
  },
  "dependencies": {
    "expo": "~40.0.0",
    "expo-status-bar": "~1.0.3",
    "native-base": "^3.4.13",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-svg": "^13.0.0",
    "react-native-web": "~0.13.12",
    "react-native-webview": "11.0.0"
  },
  "devDependencies": {
    "@types/expo": "^33.0.1",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-native": "~0.63.2",
    "@types/react-navigation": "^3.4.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-test-renderer": "16.9.4",
    "faker": "^5.1.0",
    "jest": "^26.6.3",
    "jest-coverage-badges": "1.1.2",
    "jest-expo": "40.0.1",
    "react-test-renderer": "16.13.1",
    "ts-jest": "^26.4.4",
    "typescript": "~4.0.0"
  },
  "description": "A React Native Template using Expo, Typescript and Jest",
  "keywords": [
    "expo",
    "typescript",
    "react-native",
    "with",
    "jest",
    "template",
    "react-native-template",
    "mobile"
  ],
  "homepage": "https://github.com/elgsantos/expo-template-typescript-jest#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/elgsantos/expo-template-typescript-jest.git"
  },
  "bugs": {
    "url": "https://github.com/elgsantos/expo-template-typescript-jest/issues"
  },
  "author": "Eduardo Santos",
  "license": "MIT"
}
